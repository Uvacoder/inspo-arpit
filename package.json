{
  "name": "inspo-arpit",
  "version": "1.0.0",
  "description": "",
  "main": "generatePage.js",
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "concurrently": "^5.3.0",
    "node-sass": "^5.0.0"
  },
  "devDependencies": {},
  "scripts": {
    "build": "eleventy && npm run sass",
    "serve": "npm run clean && concurrently 'eleventy --serve' 'npm run sass:watch'",
    "sass": "node-sass --output-style compressed --output _site/styles/ styles/",
    "sass:watch": "npm run sass && npm run sass -- -w",
    "clean": "rm -rf _site",
    "format": "npx prettier --write .",
    "order": "bash order.sh inspo"
  },
  "keywords": [],
